{
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "always"
    },
    "editor.showFoldingControls": "always",
    "editor.folding": true,
    "editor.foldingStrategy": "indentation",
    "scss.validate": false,
    "peacock.color": "#4c0499",
    "vsicons.presets.angular": true,
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#6505cb",
        "activityBar.background": "#6505cb",
        "activityBar.foreground": "#e7e7e7",
        "activityBar.inactiveForeground": "#e7e7e799",
        "activityBarBadge.background": "#1f0f01",
        "activityBarBadge.foreground": "#e7e7e7",
        "commandCenter.border": "#e7e7e799",
        "sash.hoverBorder": "#6505cb",
        "statusBar.background": "#4c0499",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#6505cb",
        "statusBarItem.remoteBackground": "#4c0499",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.activeBackground": "#4c0499",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#4c049999",
        "titleBar.inactiveForeground": "#e7e7e799"
    },
    "angular.enable-strict-mode-prompt": false,
    "tailwindCSS.lint.invalidConfigPath": "warning"
}
